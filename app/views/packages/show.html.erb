<%= button_to "Delete Package", @package, :method => 'delete', :data => { :confirm => "Are you sure?"} %>
<ul>
  <li>Places Include:</li>
    <% @package.places.each do |places| %>
      <ul>
        <li><%= link_to places.name, place_path(places) %></li>
      </ul>
    <% end %>
  <li>Price: <%= number_to_currency @package.price, unit: "â‚±" %></li>
</ul>
